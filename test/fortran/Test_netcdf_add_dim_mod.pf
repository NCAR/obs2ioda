module Test_netcdf_add_dim_mod
    use funit
    use netcdf_c_mod
    use netcdf_c_i_mod
    use Test_netcdf_fixture_t_mod
    implicit none
    @TestCase
    type, extends(Test_netcdf_fixture_t) :: Test_netcdf_add_dim_t
    contains
        procedure :: test_netcdfAddDim => test_netcdfAddDim
    end type
contains

    @Test
    subroutine test_netcdfAddDim(this)
        class(Test_netcdf_add_dim_t), intent(inout) :: this
        integer(c_int) :: netcdfID
        integer(c_int) :: status
        status = netcdfCreate(this%test_dim_path, netcdfID)
        call assertEqual(0, status)
        status = netcdfAddGroup(netcdfID, this%test_group_name)
        call assertEqual(0, status)
        status = netcdfAddDim(netcdfID, this%test_global_dim_name, this%test_global_dim_len)
        call assertEqual(0, status)
        status = netcdfAddDim(netcdfID, this%test_dim_name, this%test_dim_len, this%test_group_name)
        call assertEqual(0, status)
        status = netcdfClose(netcdfID)
        call assertEqual(0, status)
    end subroutine test_netcdfAddDim


end module Test_netcdf_add_dim_mod