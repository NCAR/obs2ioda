module Test_netcdf_add_group_mod
    use funit
    use netcdf_c_mod
    use netcdf_c_i_mod
    use Test_netcdf_fixture_t_mod
    implicit none
    @TestCase
    type, extends(Test_netcdf_fixture_t) :: Test_netcdf_add_group_t
    contains
        procedure :: test_netcdfAddGroup => test_netcdfAddGroup
    end type
contains
    @Test
    subroutine test_netcdfAddGroup(this)
        class(Test_netcdf_add_group_t), intent(inout) :: this
        integer(c_int) :: netcdfID
        integer(c_int) :: status
        status = netcdfCreate(this%test_group_path, netcdfID)
        call assertEqual(0, status)
        status = netcdfAddGroup(netcdfID, this%test_group_name)
        call assertEqual(0, status)
        status = netcdfAddGroup(netcdfID, this%test_nested_group_name, this%test_group_name)
        call assertEqual(0, status)
        status = netcdfClose(netcdfID)
        call assertEqual(0, status)
    end subroutine test_netcdfAddGroup

end module Test_netcdf_add_group_mod